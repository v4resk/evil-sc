int nop_decode_####UUID####(unsigned char* encoded, int length)
{
    printf("\n[-] Decrypting from NOP\n");
    int data_len = 0;
    for (int i = 0; i < length; i++) {
        if (i % 2 == 0){
            encoded[data_len] = encoded[i];
            data_len++;
        }
    }

    return data_len;
}
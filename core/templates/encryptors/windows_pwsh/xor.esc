function Invoke-Xor####UUID#### {
    param(
        [Parameter(Mandatory=$true)]
        [byte[]]$Data
    )

    Write-Host "[+] Decrypting from XOR"
    $key = [System.Text.Encoding]::UTF8.GetBytes("####KEY####")

    [byte[]]$encoded = New-Object byte[] $Data.Length
    
    for ($i = 0; $i -lt $Data.Length; $i++) {
        $encoded[$i] = [byte]($Data[$i] -bxor $key[$i % $key.Length])
    }

    return $encoded
}
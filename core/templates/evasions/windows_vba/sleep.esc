
Sub Sleep####UUID####()
    Dim startTime As Date
    Dim endTime As Date
    Dim elapsedSeconds As Double
    
    startTime = Now()
    Sleep ####SLEEP_TIME####
    endTime = Now()
    
    elapsedSeconds = DateDiff("s", startTime, endTime)
    
    If elapsedSeconds < ####VERIFY_TIME#### Then
        End
    End If
End Sub